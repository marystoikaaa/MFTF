<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">

     <section name="AdminExportSourceSection">
        <element name="exportSource" type="select" selector=".source_export_source_entity select"  timeout="200"/>
        <!--ftp file path-->
        <element name="ftpFilePath" type="input" selector=".export_source_ftp_file_path input"  timeout="200"/>
        <element name="ftpHost" type="input" selector=".export_source_ftp_host input" timeout="200" />
        <element name="ftpPort" type="input" selector=".export_source_ftp_port input"  timeout="200"/>
        <element name="ftpUser" type="input" selector=".export_source_ftp_user input"  timeout="200"/>
        <element name="ftpPassword" type="input" selector=".export_source_ftp_password input"  timeout="200"/>
    </section>
    
</sections>
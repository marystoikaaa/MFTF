<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">

    <section name="AdminStockSourcesGridSection">
        <element name="magentoStoresButton" type="button" selector="#menu-magento-backend-stores" timeout="100"/>
        <element name="stockSourcesGridButton" type="button" selector=".item-source" timeout="100" />
        <element name="stockSourcesNumberOfItems" type="text" selector=".admin__control-support-text" timeout="100" />
        <element name="stockSourcesNew" type="text" selector="//*[contains(text(), 'new-warehouse')]/../.." timeout="100" />
        <element name="stockSourcesOld" type="text" selector="//*[contains(text(), 'old-warehouse')]/../.." timeout="100" />
        
    </section>
</sections>
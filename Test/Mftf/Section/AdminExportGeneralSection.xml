<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">

    <section name="AdminExportGeneralSection">
        <element name="generalIsActive" type="button" selector=".general_is_active label" timeout="20" />
        <element name="generalTitle" type="input" selector=".general_title input"  timeout="20"/>
    </section>
    <section name="AdminExportSettingsSection">
        <element name="settingsEntity" type="select" selector=".settings_entity select"  timeout="20"/>
    </section>
    <section name="AdminExportBehaviorSection">
        <element name="behaviorBehavior" type="select" selector=".behavior_behavior_field_file_format select" timeout="20" />
    </section>
    <section name="AdminExportSourceSection">
        <element name="exportSource" type="select" selector=".source_export_source_entity select"  timeout="20"/>
        <!--ftp file path-->
        <element name="ftpFilePath" type="input" selector=".export_source_ftp_file_path input"  timeout="20"/>
        <element name="ftpHost" type="input" selector=".export_source_ftp_host input" timeout="20" />
        <element name="ftpPort" type="input" selector=".export_source_ftp_port input"  timeout="20"/>
        <element name="ftpUser" type="input" selector=".export_source_ftp_user input"  timeout="20"/>
        <element name="ftpPassword" type="input" selector=".export_source_ftp_password input"  timeout="20"/>
    </section>
    <section name="AdminExportValidateSection">
        <element name="validateButton" type="button" selector=".source_check_button" timeout="20" />
        <element name="validateResult" type="text" selector=".fieldset_source"  timeout="20"/>
    </section>
    <section name="AdminExportSaveAndRun">
        <element name="saveAndRun" type="button" selector="#save_and_run" timeout="20"/>
        <element name="runButton" type="button" selector=".run" timeout="20" />
        <element name="exportResult" type="button" selector="#debug-run" timeout="20" />
    </section>
    <section name="AdminExportMapping">
        <element name="mapContainer" type="button" selector=".source_data_map_container_source_data_count .admin__actions-switch" timeout="20" />
        <element name="addMapButton" type="button" selector=".source_data_map_rows tfoot .addButton" timeout="20" />
        <element name="mapEntity" type="select" selector=".source_data_map_source_data_entity" timeout="20" />
        <element name="sourceDataSystemOne" type="select" selector=".record-1 .source_data_map_source_data_system select" timeout="20" />
        <element name="sourceDataExportOne" type="input" selector=".record-1 .source_data_map_source_data_export input" timeout="20" />
        <element name="sourceDataSystemTwo" type="select" selector=".record-2 .source_data_map_source_data_system select" timeout="20" />
        <element name="sourceDataExportTwo" type="input" selector=".record-2 .source_data_map_source_data_export input" timeout="20" />
        <element name="sourceDataSystemThree" type="select" selector=".record-3 .source_data_map_source_data_system select" timeout="20" />
        <element name="sourceDataExportThree" type="input" selector=".record-3 .source_data_map_source_data_export input" timeout="20" />
        <element name="sourceDataMapReplaceThree" type="input" selector=".record-3 .source_data_map_source_data_replace input" timeout="20" />
    </section>

</sections>
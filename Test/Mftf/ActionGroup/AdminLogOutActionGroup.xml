<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="AdminLogOutActionGroup">

        <annotations>
            <description>Log out Admin Panel</description>
        </annotations>

        <click selector="{{AdminPanelSection.adminMenuButton}}" stepKey="clickOnAdminMenu" />
        <click selector="{{AdminPanelSection.logOutButton}}" stepKey="clickOnLogOutButton" />

    </actionGroup>

</actionGroups>